<?xml version="1.0" encoding="ISO-8859-1" ?>
<interface id="speclib_add">
    <download>
    </download>

    <!-- View definitions -->
    <view id="query_results" label="query_results">
        <blockRef id="main" type="query_results"/>
    </view>

    <!-- Block definitions -->
    <block id="query_results" type="table">
        <data>
            <source type="file" name="query_results/results.tsv"/>
            <parsers>
                <parser type="sortedTabular" sortBy="filename" operator="ascending"/>
            </parsers>

            <row>
                <column type="text" field="filename" label="filename" width="12"/>
                <column type="text" field="scan" label="scan" width="12"/>
            </row>
        </data>

    </block>
</interface>
